{
  "manifest_version": 2,
  "name": "Osrs Account Creator",
  "description": "Creates Runescape accounts",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": ["background.js", "ui.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "activeTab", "https://secure.runescape.com/*"]
}
